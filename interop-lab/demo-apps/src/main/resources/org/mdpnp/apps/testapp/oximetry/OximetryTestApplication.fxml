<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.cell.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane fx:id="main" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.mdpnp.apps.testapp.oximetry.OximetryTestApplication">
<center>
      
</center>
<right>
  <VBox>
    <children>
	    <Label>Please select an oximeter</Label>
	    <ComboBox fx:id="oximeters" promptText="choose"/>
	    <Label text="Requested Average"/>
		<TextField fx:id="requestedAverage"/>
	    <Button fx:id="setButton" text="Set"/>
	    <Label text="Current Average"/>
	    <TextField fx:id="currentAverage"/>
	    <Label text="Graph delay (ms)"/>
	    <TextField fx:id="graphDelay"/>
	    <Label text="Simple alarm threshold"/>
	    <TextField fx:id="simpleAlarmThreshold"/>
	    <Button fx:id="chooseFile" text="Select file"/>
	    <Label text="File"/>
	    <TextField fx:id="selectedFile"/>
	    <Button fx:id="runSequence" text="Run"/>
	</children>
  </VBox>
</right>
</BorderPane>

